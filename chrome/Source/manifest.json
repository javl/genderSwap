{
	"manifest_version": 2,
	"name": "genderSwap",
	"version": "1.0",
	"description": "Replaces all words referencing to men, with their female equivalent and vice versa.",
	"icons": { "16": "icon/icon_16.png",
				"48": "icon/icon_48.png",
				"128": "icon/icon_128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["genderSwap.js"],
			"run_at": "document_end"
		}
	]
}

